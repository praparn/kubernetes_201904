SIT API:

curl -X POST \
  https://api-sit.sdn.scblife.co.th/public/api/auth/pre-authentication/v1 \
  -H 'Postman-Token: 71952221-7ee0-40a1-af56-635c5adc0937' \
  -H 'cache-control: no-cache' \
  -H 'content-type: application/json' \
  -H 'x-ibm-client-id: 625297716026f33e79ef9abf189baccf' \
  -d '{
            "loginModuleId" : "E2EEPIN"
}' -k -v


curl -X POST \
  https://d2mqlapzw9vzyv.cloudfront.net/public/api/auth/pre-authentication/v1 \
